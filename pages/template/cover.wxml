<import src="common/foot.wxml" />

<template name="cover">
  <image class="start-img {{animation.startImg}}" src="{{image.start}}" mode="aspectFit" bindlongpress="longpressCover"></image>
  <view wx:if="{{resume.slogan}}" class="start-text {{animation.startText}}">
    <view class="text-left">{{resume.slogan[0]}}</view>
    <view class="text-right">{{resume.slogan[1]}}</view>
  </view>
  <template is="foot" />
</template>